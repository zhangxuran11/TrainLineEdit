QPushButton,QLabel,QLineEdit{
        font: 400 11pt bold;
}
NULL{
    border:2px groove gray;border-radius:10px;padding:2px 4px;
}

QPushButton{
        border:4px groove gray;
        border-radius:4px;
        background-color: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0,
            stop:0 rgba(123, 174, 193, 255),
            stop:0.566502 rgba(174, 214, 226, 255),
            stop:1 rgba(197, 229, 242, 255));

}
QPushButton:pressed {

     background-color: black;
     border-style: inset;
     top:-2px;
     left:-2px;
 }


QProgressBar {
     border: 2px solid grey;
     border-radius: 5px;
text-align: right;
margin-right: 4ex;
 }

 QProgressBar::chunk{
 background: red;
 }


QTableView QHeaderView::section {
    background-color:skyblue;
    color: black;
    padding-left: 4px;
    border: 1px solid #6c6c6c;
    text-align:center
}


